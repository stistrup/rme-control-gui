    "mqtt": "^5.10.3",
    "pinia": "^2.1.7",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@tauri-apps/cli": ">=2.0.0-beta.0",
    "@types/js-yaml": "^4.0.9",
    "@vitejs/plugin-vue": "^6.0.0",
    "cross-env": "^7.0.3",
    "typescript": "^5.2.2",
    "vite": "^7.0.5",
    "vue-tsc": "^2.0.22"
  }
